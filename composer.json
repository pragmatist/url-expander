{
    "name": "pragmatist/url-expander",
    "description": "A library to resolve short URLs",
    "keywords": ["url", "url expander", "short url"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/pragmatist/url-expander",
    "authors": [
        {
            "name": "Pragmatist",
            "email": "hello@pragmatist.nl",
            "homepage": "https://pragmatist.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pragmatist\\UrlExpander\\": ["src/", "tests/"]
        }
    },
    "require": {
        "php": "^7.0",
        "league/uri": "^4.2",
        "guzzlehttp/guzzle": "^6.2",
        "psr/cache": "^1.0",
        "symfony/console": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "cache/array-adapter": "^0.4.2",
        "fzaninotto/faker": "^1.6",
        "squizlabs/php_codesniffer": "^2.7"
    }
}
